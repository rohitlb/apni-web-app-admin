div.row
    div.col.s12.m12.l9.offset-l3
        img#drug(src='/images/drugsData.jpg', style='position:fixed;top:0px;left:0px;width:100%;height:100%;z-index:-1;')
        div#drugdata
            .col.s12.m12.l11
                br

                div.row
                    div.drug_alphabets.pull-left
                        span.center
                            ul
                                li
                                    a.btn2.all.hoverable(href='/health/health_care_provider?page=drug_data' type='all') All
                                li
                                    a.btn2.hoverable(href='/health/health_care_provider?page=drug_data') A
                                li
                                    a.btn2.hoverable(href='/health/health_care_provider?page=drug_data') B
                                li
                                    a.btn2.hoverable(href='/health/health_care_provider?page=drug_data') C
