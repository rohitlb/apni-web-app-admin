                                li
                                    a.btn2.hoverable(href='/health/health_care_provider?page=drug_data') F
                                li
                                    a.btn2.hoverable(href='/health/health_care_provider?page=drug_data') G
                                li
                                    a.btn2.hoverable(href='/health/health_care_provider?page=drug_data') H
                                li
                                    a.btn2.hoverable(href='/health/health_care_provider?page=drug_data') I
                                li
                                    a.btn2.hoverable(href='/health/health_care_provider?page=drug_data') J
                                li
                                    a.btn2.hoverable(href='/health/health_care_provider?page=drug_data') K
                                li
                                    a.btn2.hoverable(href='/health/health_care_provider?page=drug_data') L
                                li
                                    a.btn2.hoverable(href='/health/health_care_provider?page=drug_data') M
                                li
                                    a.btn2.hoverable(href='/health/health_care_provider?page=drug_data') N
                                li
                                    a.btn2.hoverable(href='/health/health_care_provider?page=drug_data') O
                                li
                                    a.btn2.hoverable(href='/health/health_care_provider?page=drug_data') P
                                li
                                    a.btn2.hoverable(href='/health/health_care_provider?page=drug_data') Q
                                li
                                    a.btn2.hoverable(href='/health/health_care_provider?page=drug_data') R
                                li
                                    a.btn2.hoverable(href='/health/health_care_provider?page=drug_data') S
                                li
                                    a.btn2.hoverable(href='/health/health_care_provider?page=drug_data') T
                                li
                                    a.btn2.hoverable(href='/health/health_care_provider?page=drug_data') U
                                li
                                    a.btn2.hoverable(href='/health/health_care_provider?page=drug_data') V
                                li
                                    a.btn2.hoverable(href='/health/health_care_provider?page=drug_data') W
                                li
                                    a.btn2.hoverable(href='/health/health_care_provider?page=drug_data') X
                                li
                                    a.btn2.hoverable(href='/health/health_care_provider?page=drug_data') Y
                                li
                                    a.btn2.hoverable(href='/health/health_care_provider?page=drug_data') Z
                                li
                                    a.btn2.all.hoverable(href='/health/health_care_provider?page=drug_data' type='other') Other


        div.col.s12.m12.l12
            div.col.grey.lighten-3.s12.m8.l9
                - var i = 0;
                - var tin = data.rating
                - var vas = data.brands[0]
                    h3.section.scrollspy#data_name #{vas.brand_name.toUpperCase()}
                        span <em>&nbsp - &nbsp</em>
                        each company in vas.company_id
                            span.next_line <strong>manufactured by #{company.company_name.toUpperCase()}</strong>
                        //each dosages in vas.dosage_id
                        if(vas.dosage_id[0].strength_id[0].prescription != '')
                            if(vas.dosage_id[0].strength_id[0].prescription === 'Needed')
                                span.right(style='border: groove; background-color:red; border-radius: 15px')= "Rx: "+ vas.dosage_id[0].strength_id[0].prescription
                            else if(vas.dosage_id[0].strength_id[0].prescription === 'Not Needed')
                                span.right(style='border: groove; background-color:green; border-radius: 15px')= "Rx: "+ vas.dosage_id[0].strength_id[0].prescription
                            else
                                span.right(style='border: groove; border-radius: 15px')= "Rx: "+ vas.dosage_id[0].strength_id[0].prescription

                        br

                        if(vas.categories != '')
                            h5.to_capital  [#{vas.categories}]

                    ul.collapsible(data-collapsible="expandable")
                        each dosages in vas.dosage_id
                            each doses in dosages.strength_id
                                li
                                    div.collapsible-header
                                        table.responsive-table.hide-on-down-only
                                            thead(style="background-color:lavender;")
                                                tr
                                                    th.section.scrollspy#dosage_form Dosage Form
                                                    //if(doses.strength != '')
                                                    th Strength
                                                    //if(doses.packaging != '')
                                                    th Packaging
                                                    th Price
                                                    if(dosages.dosage_form == 'Tablet')
                                                        th Price/Unit
                                            tbody
                                                tr
                                                    td <strong>#{dosages.dosage_form.toUpperCase()}</strong> &nbsp &nbsp
                                                    //if(doses.strength != '')
                                                    td <strong>#{doses.strength}</strong> &nbsp &nbsp
                                                    //if(doses.packaging != '')
                                                    td <strong>#{doses.packaging}</strong> &nbsp &nbsp
                                                    td <strong>₹ #{doses.price}</strong>
                                                    if(dosages.dosage_form == 'Tablet')
                                                        td <strong>₹ #{Number(tin[i].rateper).toFixed(2)}</strong>
                                                                //<strong>#{}</strong>
                                                        -i = i+1;
                                                tr#read_more
                                                    td
                                                    //if(doses.strength != '')
                                                    td
                                                    //if(doses.packaging != '')
                                                    td
                                                    if(dosages.dosage_form == 'Tablet')
                                                        td= '\t'
                                                    td.blue-text Read More.....

                                    div.collapsible-body
                                        span
                                            span.section.scrollspy#potent <strong>▪ Involves Potent Substances: </strong>
                                            div
                                                each j, index in doses.potent_substance.name
                                                    span.to_capital &nbsp #{j} &nbsp
                                                        span.small_font.next_line= doses.potent_substance.molecule_strength[index]
                                                    br
                                            if(doses.dose_taken != '')
                                                span.section.scrollspy#dose_taken <strong>▪ Dose Taken As:&nbsp</strong>
                                                    span.small_font #{doses.dose_taken}
                                                br
                                            if(doses.dose_timing != '')
                                                span.section.scrollspy#timing <strong>▪ Dose Timings:&nbsp</strong>
                                                    span.small_font #{doses.dose_timing}
                                                br
                                            if(vas.primarily_used_for != '')
                                                span.next_line.section.scrollspy#primarily <strong>▪ The Drug <strong>#{vas.brand_name.toUpperCase()}</strong> is primarily used for: </strong>
                                                    br
                                                    span.next_line.small_font #{vas.primarily_used_for}
                                                br
                                            if(doses.warnings != '')
                                                i.material-icons.left warning
                                                span.section.scrollspy#warnings.red-text <strong>Warnings</strong>
                                                br
                                                span.next_line #{doses.warnings}



            div.col.m4.l3.hide-on-small-only
                ul.section.table-of-contents(style='position:fixed;z-index:2;padding-bottom:10px;')
                    li
                        a(href='#data_name') Brand
                            if(vas.types != '')
                                span.to_capital -#{vas.types}
                    li
                        a(href='#prescription') Prescription
                    li
                        a(href='#potent') Potent Substances
                    li &nbsp&nbsp&nbsp&nbsp&nbspDosage Form
                            each dosage in vas.dosage_id
                                each dose in dosage.strength_id
                                    ul
                                        li
                                            a.to_capital(href='#dosage_form') &nbsp&nbsp&nbsp&nbsp#{dosage.dosage_form}
                                        if(dose.dose_taken)
                                            li
                                                a.to_capital(href='#dose_taken') &nbsp&nbsp&nbsp&nbspDose Taken for #{dosage.dosage_form}
                                        if(dose.dose_timing)
                                            li
                                                a.to_capital(href='#timing') &nbsp&nbsp&nbsp&nbspDose Timing for #{dosage.dosage_form}
                                        if(vas.primarily_used_for)
                                            li
                                                a.to_capital(href='#primarily') &nbsp&nbsp&nbsp&nbspPrimarily Used For #{dosage.dosage_form}
                                        if(dose.warnings)
                                            li
                                                a.to_capital(href='#warnings') &nbsp&nbsp&nbsp&nbspWarnings for #{dosage.dosage_form}


            div
                br
                br
                div.col.s12.l12.m12.grey.lighten-1(style='padding:5px;margin-top:20px')
                    a.black-text.modal-trigger(href='#modal2') <strong>Give a Feedback on the above data</strong>
                        i.material-icons.left.black-text feedback
            div.modal#modal2
                div.modal-content
                    h4.center.teal-text.darken-2 Your Feedback is valuable...
                    form.col.l10.m10.s10#form_feedback(method='POST' action='/health/health_care_provider?page=drug_data')
                        div
                            input#about(type="hidden" value='Drug Data-'+(vas.brand_name.toUpperCase()))
                            span How useful is the data that we provide?
                            br
                            br
                            input.with-gap#radio1(name='radio1_feedback' type='radio' value='Very Useful' checked)
                            label(for='radio1') Very Useful&nbsp
                            input.with-gap#radio2(name='radio1_feedback' type='radio' value='Useful')
                            label(for='radio2') Useful&nbsp
                            input.with-gap#radio3(name='radio1_feedback' type='radio' value='Average')
                            label(for='radio3') Average&nbsp
                            input.with-gap#radio4(name='radio1_feedback' type='radio' value='Not Useful')
                            label(for='radio4') Not Useful&nbsp
                            input.with-gap#radio5(name='radio1_feedback' type='radio' value='Not Useful At All')
                            label(for='radio5') Not Useful At All

                        br
                        div.input-field
                            span Suggest changes in data
                            textarea#suggestion(style='height:90px;' required)
                            label(for='suggestion')
                        br

                        div.row
                            div.col.s6.l4.m4.offset-l1
                                input.reset(type='reset')
                            div.col.s6.l4.m4.offset-l3
                                input.button#submit_feedback(type='submit')
